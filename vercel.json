  "version": 2,
  "buildCommand": "cd packages/web && npm run build",
  "outputDirectory": "packages/web/.next",
  "installCommand": "npm install",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./packages/web",
  "functions": {
    "packages/web/src/pages/api/**/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "framework": "nextjs"
}
